{% extends '@PrestaShop/Admin/layout.html.twig' %}
{% trans_default_domain "Module.YourModule.Admin" %}

{% block stylesheets %}
  {# include some local stylesheets #}
  <link rel="stylesheet" href="{{ asset('../modules/eniblog/views/css/app.css') }}" type="text/css" media="all">
{% endblock %}

{% block content %}
  <div id="app"></div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    <script>
      var yourModule = {{ yourModule|json_encode|raw }}
    </script>
    
    {# include some local JavaScript #}
    <script src="{{ asset('../modules/eniblog/views/js/chunk-vendors.js') }}" async defer></script>
    <script src="{{ asset('../modules/eniblog/views/js/app.js') }}" async defer></script>
{% endblock %}
